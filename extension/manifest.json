{
  "manifest_version": 3,
  "name": "Concentration Couch",
  "version": "1.0",
  "permissions": ["storage", "tabs"],
  "host_permissions": [
    "http://127.0.0.1:8000/*",
    "http://localhost:8000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "focus.png"
    }  
}
