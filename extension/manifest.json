{
  "manifest_version": 3,
  "name": "Concentration Couch",
  "version": "1.1",
  "description": "Blocks distracting websites in real time and shows productivity stats",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Concentration Couch"
  }
}
